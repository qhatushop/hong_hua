          "_customHTML": "<section class=\"footer1\" group=\"Footers\" plugins=\"amp-main, amp-form, amp-mustache\" global>\n    <!--  -->\n    <mbr-parameters>\n        <header>Size:</header>\n        <input type=\"range\" inline title=\"Top:\" name=\"paddingTop\" min=\"0\" max=\"10\" step=\"1\" value=\"8\">\n        <input type=\"range\" inline title=\"Bottom:\" name=\"paddingBottom\" min=\"0\" max=\"10\" step=\"1\" value=\"8\">\n        <!--  -->\n        <header>Columns:</header>\n        <select title=\"Title:\" name=\"alignTitle\">\n            <option value=\"1\" selected>Left</option>\n            <option value=\"2\">Center</option>\n            <option value=\"3\">Right</option>\n        </select>\n        <!--  -->\n        <header>1 column:</header>\n        <input type=\"checkbox\" title=\"Show Title\" name=\"showTitleColumn1\">\n        <input type=\"checkbox\" title=\"Show Logo:\" name=\"showLogo\" checked>\n        <input type=\"range\" title=\"Logo Width\" inline name=\"logoWidth\" min=\"55\" max=\"100\" value=\"100\" step=\"1\" condition=\"showLogo\">\n        <input type=\"checkbox\" title=\"Icon:\" name=\"showIcons\" checked>\n        <input type=\"range\" title=\"Icons:\" name=\"countSocialIcon\" min=\"0\" max=\"6\" step=\"1\" value=\"1\" condition=\"showIcons\">\n        <input type=\"range\" title=\"Size Icon:\" name=\"sizeIcon\" min=\"1\" max=\"2\" step=\"0.1\" value=\"1\" condition=\"showIcons\">\n        <input type=\"color\" title=\"Color Icon:\" name=\"colorIcon\" value=\"#000000\" condition=\"showIcons\">\n        <input type=\"color\" title=\"BG Color Icon:\" name=\"colorBgIcon\" value=\"#f3f3f3\" condition=\"showIcons\">\n        <!--  -->\n        <header>2 column:</header>\n        <input type=\"checkbox\" title=\"Show Title\" name=\"showTitleColumn2\" checked>\n        <input type=\"range\" title=\"Strings:\" name=\"countStringCol1\" min=\"0\" max=\"8\" step=\"1\" value=\"2\">\n        <select title=\"Align:\" name=\"alignCol1\">\n            <option value=\"1\" selected>Left</option>\n            <option value=\"2\">Center</option>\n            <option value=\"3\">Right</option>\n        </select>\n        <!--  -->\n        <header>3 column:</header>\n        <input type=\"checkbox\" title=\"Show Title\" name=\"showTitleColumn3\" checked>\n        <input type=\"range\" title=\"Amount string:\" name=\"countStringCol2\" min=\"0\" max=\"8\" step=\"1\" value=\"2\">\n        <select title=\"Align\" name=\"alignCol2\">\n            <option value=\"1\" selected>Left</option>\n            <option value=\"2\">Center</option>\n            <option value=\"3\">Right</option>\n        </select>\n        <!--  -->\n        <header>4 column:</header>\n        <input type=\"checkbox\" title=\"Show column\" name=\"showColumn4\" checked>\n        <input type=\"checkbox\" title=\"Show Title\" name=\"showTitleColumn4\" condition=\"showColumn4\" checked>\n        <!--  -->\n        <header>Background:</header>\n        <fieldset type=\"background\" name=\"bg\">\n            <input type=\"image\" title=\"Background Image:\" value=\"../_images/images/1.jpg\">\n            <input type=\"color\" title=\"Background Color:\" value=\"#009f28\" selected>\n        </fieldset>\n        <input type=\"checkbox\" title=\"Overlay:\" name=\"overlay\" checked condition=\"bg.type!='color'\">\n        <input type=\"color\" title=\"Overlay Color:\" name=\"overlayColor\" value=\"#ffffff\" condition=\"bg.type!='color' && overlay\">\n        <input type=\"range\" inline title=\"Opacity:\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.05\" value=\"0.95\" condition=\"bg.type!='color' && overlay\">\n    </mbr-parameters>\n    <!--  -->\n    <!-- Overlay -->\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type !== 'color'\"></div>\n    <!-- Container -->\n    <div class=\"container\">\n        <!-- Content -->\n        <div class=\"mbr-row mbr-jc-c\">\n            <!-- 1-column -->\n            <div class=\"mbr-col-sm-12 mbr-col-md-6 mbr-col-lg-4\" mbr-class=\"{'mbr-col-lg-6' : !showColumn4}\">\n                <div class=\"column column1\">\n                    <!-- Title-column\t -->\n                    <h5 class=\"mbr-fonts-style mbr-semibold title-column\" mbr-theme-style=\"display-4\" mbr-if=\"showTitleColumn1\" mbr-class=\"{'align-left' : alignTitle == 1, 'align-center' : alignTitle == 2, 'align-right' : alignTitle == 3}\">\n                        Features\n                    </h5>\n                    <!--Logo-->\n                    <span mbr-if=\"showLogo\" class=\"column1-logo\">\n                      <amp-img src=\"@PROJECT_PATH@/assets/images/logotipo-200x151.png\" layout=\"responsive\" width=\"100.66225165562915\" height=\"76\" alt=\"Mobirise\" class=\"mobirise-loader\">\n                          <div placeholder class=\"placeholder\"></div>\n                          <a href=\"index.html\"><img class=\"mbr-temp\" src=\"@PROJECT_PATH@/assets/images/logotipo-200x151.png\" alt=\"Mobirise\" title></a>\n                      </amp-img>\n                    </span>\n                    <!--Text-->\n                    <p class=\"mbr-fonts-style mbr-light column1-text\" mbr-theme-style=\"display-4\" data-app-selector=\".column1-text\">\n                        Alta calidad que cuida tus manos y las de tu familia...</p>\n                    <!--Icons-->\n                    <div class=\"social-wrap mbr-pt-3\" mbr-list mbr-list-grow=\"countSocialIcon\">\n                        <a href=\"https://www.facebook.com/Mobirise\" target=\"_blank\" class=\"social\" mbr-if=\"showIcons\">\n\t\t\t\t\t\t\t<span class=\"iconfont-wrapper\">\n\t\t\t\t\t\t\t\t<span mbr-icon class=\"amp-iconfont fa-facebook fa\"></span>\n\t\t\t\t\t\t\t</span>\n                        </a>\n                        \n                        \n                        \n                    </div>\n                </div>\n            </div>\n            <!-- 2-column -->\n            <div class=\"mbr-col-sm-12 mbr-col-md-3 mbr-col-lg-2\" mbr-class=\"{'mbr-col-lg-3' : !showColumn4}\" mbr-if>\n                <div class=\"column column2\">\n                    <!-- Title-column\t -->\n                    <h5 class=\"mbr-fonts-style mbr-semibold title-column\" mbr-theme-style=\"display-4\" mbr-if=\"showTitleColumn2\" mbr-class=\"{'align-left' : alignTitle == 1, 'align-center' : alignTitle == 2, 'align-right' : alignTitle == 3}\">\n                        Contacto</h5>\n                    <!-- Content-column -->\n                    <div mbr-list mbr-list-grow=\"countStringCol1\" class=\"b-links-1\" mbr-class=\"{'align-left' : alignCol1 == 1, 'align-center' : alignCol1 == 2, 'align-right' : alignCol1 == 3}\">\n                        <p class=\"mbr-fonts-style mbr-regular mbr-pb-2 column2-item\" mbr-theme-style=\"display-4\" data-app-selector=\".column2-item\">Cel 995574393</p>\n                    <p class=\"mbr-fonts-style mbr-regular mbr-pb-2 column2-item\" mbr-theme-style=\"display-4\" data-app-selector=\".column2-item\"><a href=\"https://honghua.com\" class=\"text-white\">honghua.com</a></p></div>\n                </div>\n            </div>\n            <!-- 3-column -->\n            <div class=\"mbr-col-sm-12 mbr-col-md-3 mbr-col-lg-2\" mbr-class=\"{'mbr-col-lg-3' : !showColumn4}\" mbr-if>\n                <div class=\"column column3\">\n                    <!-- Title-column\t -->\n                    <h5 class=\"mbr-fonts-style mbr-semibold title-column\" mbr-theme-style=\"display-4\" mbr-if=\"showTitleColumn3\" mbr-class=\"{'align-left' : alignTitle == 1, 'align-center' : alignTitle == 2, 'align-right' : alignTitle == 3}\">\n                        Enlaces</h5>\n                    <!-- Content-column -->\n                    <div mbr-list mbr-list-grow=\"countStringCol2\" class=\"b-links-1\" mbr-class=\"{'align-left' : alignCol2 == 1, 'align-center' : alignCol2 == 2, 'align-right' : alignCol2 == 3}\">\n                        <p class=\"mbr-fonts-style mbr-regular mbr-pb-2 column3-item\" mbr-theme-style=\"display-4\" data-app-selector=\".column3-item\"><a href=\"page2.html\" class=\"text-white\">Shop</a></p>\n                    <p class=\"mbr-fonts-style mbr-regular mbr-pb-2 column3-item\" mbr-theme-style=\"display-4\" data-app-selector=\".column3-item\"><a href=\"index.html#features2-2\" class=\"text-white\">Nosotros</a></p></div>\n                </div>\n            </div>\n            <!-- 4-column -->\n            <div class=\"mbr-col-sm-12 mbr-col-md-12 mbr-col-lg-4\" mbr-if=\"showColumn4\">\n                <div class=\"column column4\">\n                    <!-- Title-column\t -->\n                    <h5 class=\"mbr-fonts-style mbr-semibold title-column\" mbr-theme-style=\"display-4\" mbr-if=\"showTitleColumn4\" mbr-class=\"{'align-left' : alignTitle == 1, 'align-center' : alignTitle == 2, 'align-right' : alignTitle == 3}\">\n                        Suscríbete</h5>\n                    <!-- Form -->\n                    <div mbr-form class=\"column4__form\">\n                        <form action=\"mguevararojas@gmail.com\" action-xhr name=\"Mobirise Form\" method=\"POST\" class=\"mbr-form\">\n                            <div class=\"mbr-form-row\">\n                                <div submit-success class=\"mbr-col-lg-12 mbr-col-md-12 mbr-col-sm-12\">\n                                    <template data-form-alert type=\"amp-mustache\">Suscrito satisfactoriamente!</template>\n                                </div>\n                                <div submit-error class=\"mbr-col-lg-12 mbr-col-md-12 mbr-col-sm-12\">\n                                    <template data-form-alert type=\"amp-mustache\">Suscrito satisfactoriamente!</template>\n                                </div>\n                            </div>\n                            <div class=\"mbr-form-row\">\n                                <div data-for=\"form[data][0][1]\" class=\"column4__form__input\">\n                                    <input type=\"hidden\" name=\"form[data][0][0]\" value=\"Enter your email here\">\n                                    <input type=\"email\" name=\"form[data][0][1]\" placeholder=\"Ingresa tu email\" data-form-field=\"Email\" mbr-theme-style=\"display-4\" required=\"required\" class=\"field-input\">\n                                    <div mbr-buttons=\"true\" mbr-theme-style=\"display-4\" data-toolbar=\"-mbrBtnMove,-mbrBtnLink,-mbrBtnAdd\" class=\"mbr-fonts-style column4__form__btn\"><a type=\"submit\" class=\"btn-primary btn column4__form__btn__text\" data-app-placeholder=\"Type Text\">SUSCRIBETE</a></div>\n                                </div>\n                            </div>\n                        </form>\n                    </div>\n                </div>\n            </div>\n        </div>\n    </div>\n</section>",
